config_version=5

[application]

config/name="DiceRoller2000"
config/description="A random number generator.
Perfect for roleplay games like Shadowrun or D&D.
Designed for situations where you have to roll lots of dices."
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.0")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
config/icon="res://logo.png"
config/windows_native_icon="res://icon.ico"

[debug]

settings/gdscript/max_call_stack=4000

[rendering]

renderer/rendering_method="gl_compatibility"
environment/defaults/default_clear_color=Color(1, 1, 1, 1)
environment/defaults/default_environment="res://default_env.tres"
quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
